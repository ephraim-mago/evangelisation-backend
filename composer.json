{
    "name": "ephraim-mago/evangelisation-backend",
    "type": "project",
    "description": "The API for evangelisation app",
    "keywords": [
        "framework",
        "api"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ephra√Øm Mago",
            "email": "ephraimmago@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-ctype": "*",
        "ext-filter": "*",
        "ext-hash": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-session": "*",
        "ext-tokenizer": "*",
        "composer-runtime-api": "^2.2",
        "doctrine/inflector": "^2.1",
        "filp/whoops": "^2.18",
        "guzzlehttp/psr7": "^2.8",
        "nikic/fast-route": "^1.3",
        "psr/container": "^2.0",
        "symfony/var-dumper": "^7.2",
        "vlucas/phpdotenv": "^5.6",
        "vlucas/valitron": "^1.4",
        "voku/portable-ascii": "^2.0"
    },
    "provide": {
        "psr/container-implementation": "2.0"
    },
    "autoload": {
        "files": [
            "src/Framework/Collection/helpers.php",
            "src/Framework/Filesystem/functions.php",
            "src/Framework/Core/helpers.php",
            "src/Framework/Support/helpers.php"
        ],
        "psr-4": {
            "Framework\\": "src/Framework/",
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "Framework\\Tests\\": "tests/"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
